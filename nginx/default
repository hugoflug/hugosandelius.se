server {
    listen 80;

    location / {
        alias /site/;
    }

    location /static {
        alias /static/;
    }

    location /hatify {
        proxy_pass http://hatify;
        proxy_set_header Host $host;
    }

    location /musclify {
        proxy_pass http://hatify;
        proxy_set_header Host $host;
    }

    location /tagga {
        proxy_pass http://tagga;
        proxy_set_header Host $host;
    }

    location /1337 {
        proxy_pass http://leet;
        proxy_set_header Host $host;
    }
}
